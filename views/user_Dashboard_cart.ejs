<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <title>User Dashboard</title>
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.0/css/all.min.css" />
  <link rel="stylesheet" href="./CSS/cart.css" />
  <link rel="stylesheet" href="./CSS/payment1.css" />
  <link rel="stylesheet" href="./CSS/user_Dashboard.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>

<body>
  <!-- partial:index.partial.html -->
  <div class="dashboard">
    <%- include('user_dashboard_navbar.ejs'); %>

    <div class="dashboard-app">
      <header class="dashboard-toolbar">
        <a href="#!" class="menu-toggle"><i class="fas fa-bars"></i></a>
        <a href="#" class="menu-toggle">
          <i class="fas fa-right-from-bracket"></i>
          Logout
        </a>
      </header>





      <!-- User Profile  -->
      <div class="dashboard-content" id="dashboard-review-page">
        <div class="container">
          <!-- <div class="container-review"> -->
               <!-- <h2>FitWell cart</h2> -->
            
        <div class="parent-container">
            <div class="cart-container">
              <table>
                <tr>
                  <th>Product</th>
                  <th>Quantity</th>
                  <th>Subtotal</th>
                </tr>
                <hr />
                <tr>
                  <td>
                    <div class="cart-info">
                      <img src="./img/product_2-300x300.jpg" />
                      <div>
                        <p>Nitro Tech + ISO PRO <span>(combo offer)</span></p>
                        <small>price :$35</small><br />
                        <a href="#">Remove</a>
                      </div>
                    </div>
                  </td>
                  <td><input type="number" value="1" /></td>
                  <td>$35</td>
                </tr>

                <tr>
                  <td>
                    <div class="cart-info">
                      <img src="./img/product_1-300x300.jpg" />
                      <div>
                        <p>SYNTHA-6</p>
                        <small>price :$40</small><br />
                        <a href="#">Remove</a>
                      </div>
                    </div>
                  </td>
                  <td><input type="number" value="1" /></td>
                  <td>$40</td>
                </tr>
              </table>
            </div>

            <div class="total-price">
              <h3>Price Details</h3>
              <table>
                <tr>
                  <td>Sub total</td>
                  <td>$75.00</td>
                </tr>
                <tr>
                  <td>Discount 5%</td>
                  <td>$5.00</td>
                </tr>
                <tr>
                  <td>Delivery charges</td>
                  <td>$5.00</td>
                </tr>
                <tr>
                  <td>Total Amount</td>
                  <td>$70.00</td>
                  <br /><br />
                </tr>
                <td>
                  <input
                    id="checkbut"
                    type="button"
                    class="w3-button w3-black"
                    value="Check Out"
                  />
                </td>
              </table>
            </div>
          </div>
          <a id="back" href="/products">
            <h4>â¬… Go back to Continue Shopping</h4>
          </a> 
          </div>
        </div>
      </div>
    </div>

    <!-- Button trigger modal -->
<button type="button" id="modalbutton" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter">
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <section id="payment">
          <div class="container">
              <div class="column-two">
                  <table class="details">
                      <tr class="last">
                          <th colspan="2">Review Your Details:</th>
                      </tr>
                      <tr>
                          <td>Name:</td>
                          <td>XYZ</td>
                      </tr>
                      <tr>
                          <td>Amount:</td>
                          <td><i class="fi-xnsuxm-rupee"></i> 24,000</td>
                      </tr>
                      <tr>
                          <td>GST(18%):</td>
                          <td><i class="fi-xnsuxm-rupee"></i> 5,400</td>
                      </tr>
  
                      <tr class="last">
                          <td>Total Amount:</td>
                          <td><i class="fi-xnsuxm-rupee"></i> 30,000</td>
                      </tr>
                  </table>
  
                  <table class="payment-method">
                      <tr class="last">
                          <th>Select Your Payment Method:</th>
                      </tr>
  
                      <tr>
                          <td>
                              <input type="radio" name="payment-method" id="payment-method">
                              <label for="payment-method">Net Banking/Credit/Debit Card</label>
                          </td>
                      </tr>
  
                      <tr>
                          <td>NOTE: After clicking the "Sure, Get me Started" button below, you will be redirected to the
                              secure
                              page of our online payment service partner.</td>
                      </tr>
                  </table>
  
                  <div class="buttons">
                      <a href="/user_Dashboard_home"><button class="sumbit" type="button">Sure, Get me Started</button></a>
                      
                  </div>
                    <!-- <div class="payment-options">
                        <p>Important Note:* Your order is incomplete unless you click on the "Sure, Get me Started" button
                            above.</p>
                        <h2>PAYMENT OPTIONS</h2>
                        <img src="Images\flp-visa.png" alt="">
                        <img src="Images\flp-master-card.png" alt="">
                        <img src="Images\flp-maestro.png" alt="">
                        <img src="Images\flp-american.png" alt="">
                        <img src="Images\flp-net-banking.png" alt="">
                    </div> -->
              </div>
  
          </div>
      </section>
      </div>
      <!-- <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button> -->
      </div> 
    </div>
  </div>
</div>
    <!-- partial -->

    <script>
      let checkout=document.getElementById('checkbut');
      let modalbutton=document.getElementById('modalbutton');
      checkout.addEventListener('click',()=>{
          modalbutton.click();
      })
    </script>
    <script src="https://kit.fontawesome.com/1b34bcf330.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="./JS/user_Dashboard.js"></script>
</body>

</html>